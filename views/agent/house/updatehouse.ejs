<%- include('../partials/header') %>
        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">
            <div class="container-fluid">
                <div class="row page-titles mx-0">
                    <div class="col-sm-6 p-md-0">
                        <div class="welcome-text">
                            <h4>Update House Information</h4>
                        </div>
                    </div>
                    <div class="col-sm-6 p-md-0 justify-content-sm-end mt-2 mt-sm-0 d-flex">
                        <a href="/agent/house/<%- house._id %>" class="btn btn-primary">View House</a>
                    </div>
                </div>
                <!-- row -->
                <div class="row">
                    <div class="col-xl-6 col-xxl-12">

                        <div class="card">
                            <div class="card-body">
                                <div class="basic-form">
                                    <form method="POST" action="/agent/house/<%- house._id %>?_method=PUT">
                                        <div class="form-row">
                                            <div class="form-group col-md-6">
                                                <label>Address</label>
                                                <input type="text" class="form-control" value="<%- house.address %>" name="address">
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label>Price</label>
                                                <input type="String" class="form-control" value="<%- house.price %>" name="price">
                                            </div>
                                        </div>
                                        <div class="form-row">
                                            <div class="form-group col-md-6">
                                                <label>Type</label>
                                                <select id="inputState" class="form-control" name="type">
                                                    <option selected disabled>Choose House Type</option>
                                                    <option value="Rent">Rent</option>
                                                    <option value="Lease">Lease</option>
                                                    <option value="Sale">Sale</option>
                                                </select>
                                            </div>
                                            <div class="form-group col-md-6">
                                                <label>Duration</label>
                                                <select id="inputState" class="form-control" name="duration">
                                                    <option selected disabled>Choose Duration</option>
                                                    <option value="1 Year">1 Year</option>
                                                    <option value="2 Years">2 Years</option>
                                                    <option value="3 Years">3 Years</option>
                                                </select>
                                            </div>  
                                        </div>
                                        <div class="card">
                                            <label>House Description</label>
                                            <div class="form-group">
                                                <textarea class="form-control" rows="4" cols="200" id="comment" name="description"><%- house.description %></textarea>
                                            </div>
                                        </div>

                                        <button type="submit" class="btn btn-primary">Update House Information</button>
                                    </form>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
        <!--**********************************
            Content body end
        ***********************************-->
<%- include('../partials/footer') %>